RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

RemoveHandler .html .htm 
AddType application/x-httpd-php .php .htm .html .phtml

<FilesMatch "\.(php)$">
Order allow,deny
Allow from all
</FilesMatch>

<Directory /uploads>
Options +Indexes
Satisfy Any
Allow from all
</Directory>
